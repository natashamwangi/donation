<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="#96000000" android:endColor="#96000000" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#ffffffff" />
            <corners android:radius="0.0dip" />
            <padding android:left="10.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="#96000000" android:endColor="#96000000" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#ffffffff" />
            <corners android:radius="0.0dip" />
            <padding android:left="10.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="#96ffffff" android:endColor="#96ffffff" android:angle="270.0" />
            <stroke android:width="1.0dip" android:color="#ff000000" />
            <corners android:radius="0.0dip" />
            <padding android:left="5.0dip" />
        </shape>
    </item>
</selector>